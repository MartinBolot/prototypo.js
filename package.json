{
  "name": "prototypo.js",
  "version": "0.0.1",
  "description": "parametric-glyph engine",
  "dependencies": {
  },
  "devDependencies": {
    "plumin.js": "^0.1.1",
    "browserify": "^6.2.0",
    "watchify": "^2.1.1",
    "exorcist": "^0.1.6",
    "uglify-js": "^2.4.14",
    "mocha": "^2.0.1",
    "chai": "^1.10.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "dist/prototypo.js",
